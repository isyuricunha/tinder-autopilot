{
  "name": "Autopilot for Tinder",
  "short_name": "Tinder Auto",
  "homepage_url": "https://github.com/isyuricunha/tinder-autopilot/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Don't waste any more time doing manual tasks on Tinder. Autopilot will do it for you.",
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.0.0",
  "author": "isyuricunha",
  "background": {
    "scripts": ["bg.js"],
    "persistent": false
  },
  "permissions": [
    "storage",
    "https://api.gotinder.com/*",
    "https://*.gotinder.com/*"
  ],
  "content_scripts": [
    {
      "js": ["main.js"],
      "matches": ["https://tinder.com/*", "https://*.gotinder.com/*"]
    }
  ]
}
